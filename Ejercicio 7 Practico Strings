#include <iostream>
#include <string>
using namespace std;

int main(){
    string s;
    getline(cin, s);

    // quitar espacios
    string t = "";
    for(char c : s)
        if(!isspace(c)) t += c;

    // ordenamiento por selecci√≥n
    for(int i=0; i<t.size(); i++){
        int minpos = i;
        for(int j=i+1; j<t.size(); j++){
            if(t[j] < t[minpos])
                minpos = j;
        }
        swap(t[i], t[minpos]);
    }

    cout << t;
    return 0;
}
