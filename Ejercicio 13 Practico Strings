#include <iostream>
#include <string>
using namespace std;

int main(){
    string a, b;
    getline(cin, a);
    getline(cin, b);

    int n = a.size(), m = b.size();

    for(int i=0; i+n <= m; i++){
        bool ok = true;
        for(int j=0; j<n; j++){
            if(b[i+j] != a[j]){
                ok = false;
                break;
            }
        }
        if(ok){
            cout << i+1; // 1-based
            return 0;
        }
    }

    cout << -1;
    return 0;
}
