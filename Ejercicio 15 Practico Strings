#include <iostream>
#include <string>
#include <cctype>
using namespace std;

int main(){
    string s;
    getline(cin, s);

    string t = "";
    for(char c : s){
        if(isalpha(c)) t += tolower(c);
    }

    int i=0, j=t.size()-1;
    while(i < j){
        if(t[i] != t[j]){
            cout << "false";
            return 0;
        }
        i++; j--;
    }

    cout << "true";
    return 0;
}
