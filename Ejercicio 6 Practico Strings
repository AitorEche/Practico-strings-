#include <iostream>
#include <string>
#include <cctype>
using namespace std;

bool esVocal(char c){
    c = tolower(c);
    return c=='a'||c=='e'||c=='i'||c=='o'||c=='u';
}

int main(){
    string s;
    getline(cin, s);

    string r = "";
    for(char c : s)
        if(!esVocal(c))
            r += c;

    cout << r;
    return 0;
}
